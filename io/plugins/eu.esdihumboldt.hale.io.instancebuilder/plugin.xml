<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="eu.esdihumboldt.hale.io.provider">
      <provider
            allowDuplicate="false"
            class="eu.esdihumboldt.hale.io.instancebuilder.InstanceBuilderReader"
            description="Reads and instance or instances from an instance builder script. Intended for testing purposes."
            id="eu.esdihumboldt.hale.io.instancebuilder"
            name="Instance builder script [unsafe]">
         <contentType
               ref="eu.esdihumboldt.hale.io.instancebuilder">
         </contentType>
      </provider>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            file-extensions="instancebuilder,groovy"
            id="eu.esdihumboldt.hale.io.instancebuilder"
            name="Instance builder script"
            priority="normal">
      </content-type>
   </extension>

</plugin>
